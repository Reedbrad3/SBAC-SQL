select distinct
  a.studentID,
  a.FLEID,
  b.otherID,
  b.stateCode,
  b.tchrLastName,
  b.tchrFirstName,
  b.tchralphaKey,
  d.ssn,
  d.localStaffID,
  c.entityID,
  c.grade

  
from
  ala_survey3scs2122  a
  join ala_roster_history b on b.fleID = a.FLEID and b.schoolYear = '2022' and b.tchrType = 'P'
  join ala_allStudents_2122_eoy c on c.fleid = a.FLEID and (c.wdDate > '2/11/2022' or c.wdDate IS NULL) and c.entryDate < '2/11/2022'
  join allStaff d on d.alphaKey = b.tchralphaKey
