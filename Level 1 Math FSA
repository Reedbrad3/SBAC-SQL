#Level 1 FSA MATH
select distinct
  t.schl,
  s.[school-name],
  t.grade,
  count(distinct t.studentID) as 'count'
from
  ala_testScore t
  join school   s on s.[school-id] = t.schl
where
  t.testName = 'FSA Math'
  and [date] between '2022-05-01' and '2022-06-30'
  and al = '1'
group by
  t.schl,
  s.[school-name],
  t.grade
  t.studentID
