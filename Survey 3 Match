select distinct
  a.studentID,
  a.FLEID,
  b.otherID,
  b.stateCode,
  b.tchrLastName,
  b.tchrFirstName,
  b.alphaKey

  
from
  ala_survey2scs2122  a
  join ala_roster_history b on b.fleID = a.FLEID and b.schoolYear = '2022'
  join ala_allStudents_2122_eoy c on c.fleid = a.FLEID and (c.wdDate > '10/14/2021' or c.wdDate IS NULL)
