select distinct
  a.[Student ID],
  a.Fleid,
  a.Entityid,
  b.[First Name],
  b.[Last Name],
  c.dob,
  c.grade,
  c.entityName,
  b.[Start Term],
  b.[Section Number],
  b.Period,
  b.[Sched Method],
  b.[Course ID],
  b.[Course Section],
  b.[State Code],
  b.[Tchr Last Name],
  b.[Tchr First name],
  d.alphaKey
  
from
  ala_survey2scs2122  a
  join ala_roster_history b on b.[studentID] = a.[Student ID]
  join ala_allStudents_2122_eoy    c on c.studentID = a.[Student ID]
  join activeStaff d on  d.alphaKey = b.[Tchr Alpha Key]


group by
  a.[Student ID],
  a.Fleid,
  a.Entityid,
  b.[First Name],
  b.[Last Name],
  c.dob,
  c.grade,
  c.entityName,
  b.[Start Term],
  b.[Section Number],
  b.Period,
  b.[Sched Method],
  b.[Course ID],
  b.[Course Section],
  b.[State Code],
  b.[Tchr Last Name],
  b.[Tchr First name],
  d.alphaKey
