select distinct
  a.studentID,
  a.Fleid,
  a.Entityid, 
  b.firstName,
  b.lastName,
  c.dob,
  c.grade,
  c.entityName,
  b.startTerm,
  b.sectionNumber,
  b.Period,
  b.schedMethod,
  b.courseID,
  b.courseSection,
  b.stateCode,
  b.tchrLastName,
  b.tchrFirstName,
  d.alphaKey

  
from
  ala_survey2scs2122  a
  join ala_roster_history b on b.studentID = a.studentID
  join ala_allStudents_2122_eoy    c on c.studentID = a.studentID
  join activeStaff d on  d.alphaKey = b.tchrAlphaKey


group by
    a.studentID,
  a.Fleid,
  a.Entityid, 
  b.firstName,
  b.lastName,
  c.dob,
  c.grade,
  c.entityName,
  b.startTerm,
  b.sectionNumber,
  b.Period,
  b.schedMethod,
  b.courseID,
  b.courseSection,
  b.stateCode,
  b.tchrLastName,
  b.tchrFirstName,
  d.alphaKey
