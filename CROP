select distinct
  a.studentID,
  a.entityName,
  a.grade,
  b.unweightedCumGPA,
  b.weightedCumGPA,
  c.courseName,
  c.sem2Grade,
  c.FG,
  d.[absence-days],
  d.[attendance-days]
 
from
  ala_activeStudents  a 
  left join ala_activeStudents_2122_eoy     b on b.studentID = a.studentID
  left join ala_roster_history    c on c.studentID = a.studentID
  left join [student-ew]          d on d.[student-ID] = a.studentID and [withdrawal-school-year] = 2022
where
  a.grade in ('06','07','08')
